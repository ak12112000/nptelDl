{
	"manifest_version":2,
	"name":"nptelDl",
	"description": "Download nptel courses in one click.",
	"version":"1.0",
	"icons":{
		"64":"icon.png"
	},
	"background":{
		"scripts":["jquery.js","background.js"]
	},
	"browser_action": {
	    "default_icon": "icon.png",
	    "default_popup": "popup.html"
  	},
	"permissions":[
		"http://nptel.ac.in/",
		"downloads"
	]
}