{
	"manifest_version":2,
	"name":"nptelDl",
	"description": "Download nptel video courses in one click.",
	"version":"1.1",
	"icons":{
		"16":"icon.png",
		"32":"icon.png",
		"64":"icon.png",
		"128":"icon.png"
	},
	"background":{
		"scripts":["jquery.js","background.js"]
	},
	"browser_action": {
	    "default_icon": "icon.png",
	    "default_popup": "popup.html"
  	},
  	"web_accessible_resources":["style.css"],
	"permissions":[
		"http://nptel.ac.in/",
		"downloads"
	]
}